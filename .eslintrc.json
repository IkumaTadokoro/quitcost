{
    "env": {
        "node": true,
        "es2021": true,
        "jest/globals": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:vue/vue3-essential",
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": 13,
        "sourceType": "module"
    },
    "plugins": [
        "vue",
        "jest"
    ],
    "rules": {
        "vue/multi-word-component-names": 0
    },
    "globals": {
        "$ref": "readonly",
        "$$": "readonly",
        "$computed": "readonly",
        "defineProps": "readonly",
        "defineEmits": "readonly",
        "defineExpose": "readonly",
        "withDefaults": "readonly"
    }
}
